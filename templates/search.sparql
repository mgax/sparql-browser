{%- macro filter(name, value, regex) -%}
  {%- if value -%}
    {%- if regex -%}
      FILTER(regex(str(?{{ name }}), "{{ value }}"))
    {%- else -%}
      FILTER(?{{ name }} = <{{ value }}>)
    {%- endif -%}
  {%- endif -%}
{%- endmacro -%}


SELECT DISTINCT * WHERE {

  ?s ?p ?o .

  {{ filter('s', s, s_regex) }}
  {{ filter('p', p, p_regex) }}
  {{ filter('o', o, o_regex) }}

}

LIMIT 100
